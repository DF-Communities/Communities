    <validationRules>
        <fullName>Complete_opening_times</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
    NOT(ISNEW()),
    OR(
       AND(
           ISBLANK(Opening_times_Other__c),
           ISBLANK( Text(Recurrence__c) )
       ),
       AND(
           NOT(ISBLANK(Opening_times_Other__c)),
           NOT(ISBLANK( Text(Recurrence__c) ))
       )
    )
)</errorConditionFormula>
        <errorMessage>Please fill in either of the Opening Times options A or B. You can also include additional information in Option C if you wish.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Ensure_Service_is_linked_to_org_with_DCE</fullName>
        <active>true</active>
        <description>This validation rule ensures that services are linked to Organisations which have a DCE ID filled in</description>
        <errorConditionFormula>AND(
    OR(
       ISNEW(),
       ISCHANGED( DCEPartner__c )
    ),
    OR(
       ISBLANK( DCEPartner__c ),
       ISBLANK( DCEPartner__r.DCEProdOrgID__c ),
       NOT(BEGINS(DCEPartner__r.DCEProdOrgID__c, &quot;001&quot;))
    )
)</errorConditionFormula>
        <errorDisplayField>DCEPartner__c</errorDisplayField>
        <errorMessage>Please ensure this service is linked to an organisation record which has been linked to an External Organisation in CRS</errorMessage>
    </validationRules>
